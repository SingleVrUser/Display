<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converter="using:Display.Converter"
    xmlns:communityConverters="using:CommunityToolkit.WinUI.UI.Converters">

    <converter:BytesToSizeStrConverter x:Key="BytesToSizeStrConverter" />
    <converter:NumberToDateTimeStrConverter x:Key="NumberToDateTimeStrConverter" />
    <converter:NumberToLengthStrConverter x:Key="NumberToLengthStrConverter" />
    <converter:OneToTrueConverter x:Key="OneToTrueConverter" />
    <converter:AppUpdateContentConverter x:Key="AppUpdateContentConverter" />
    <converter:NullToCollapsedConverter x:Key="NullToCollapsedConverter" />
    <converter:DoubleToInt32Converter x:Key="DoubleToInt32Converter" />
    <converter:ObjectToColorConverter x:Key="ObjectToColorConverter" />
    <converter:NotExistsImageBitmapConverter x:Key="NotExistsImageBitmapConverter" />
    <converter:NotExistsImageUriConverter x:Key="NotExistsImageUriConverter" />
    <converter:TrueToVisibleConverter x:Key="TrueToVisibleConverter" />
    <converter:ZeroToVisibleConverter x:Key="ZeroToVisibleConverter" />
    <converter:FloatToPercentageConverter x:Key="FloatToPercentageConverter" />
    <converter:TimeStampToStringConverter x:Key="TimeStampToStringConverter" />
    <communityConverters:StringVisibilityConverter x:Key="StringVisibilityConverter"/>
    <communityConverters:BoolNegationConverter x:Key="BoolNegationConverter" />
    <communityConverters:FileSizeToFriendlyStringConverter x:Key="FileSizeToFriendlyStringConverter" />
    <communityConverters:DoubleToVisibilityConverter x:Key="GreaterThanZeroToVisibilityConverter" GreaterThan="0" />

</ResourceDictionary>
